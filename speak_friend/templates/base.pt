<html xmlns:tal="http://xml.zope.org/namespaces/tal">
    <head>
        <link rel="stylesheet" href="/static/css/form.css" type="text/css" />
        <script src="/static/scripts/jquery-1.4.2.min.js"></script>
        <script src="/static/scripts/deform.js"></script>
    </head>
    <body>
        <div id="header">
            <metal:content define-slot="header" />
        </div>
        <div id="breadcrumbs">
            <metal:content define-slot="breadcrumbs" />
        </div>
        <div id="content">
            <metal:content define-slot="content" />
        </div>
        <div id="footer">
            <metal:content define-slot="footer" />
        </div>

        <!--! Javascript includes that don't absolutely have to be in the head, for page speed -->
        <script>
            deform.load()
        </script>
    </body>
</html>
